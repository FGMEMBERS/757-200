	<!-- APU knob -->
	<animation>
		<name>APU knob pick</name>
		<type>pick</type>
		<object-name>APUKnb</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/APU/off-start-run</property>
				<step type="int">1</step>
				<max>3</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/APU/off-start-run</property>
				<step type="int">-1</step>
				<min>0</min>
			</binding>
		</action>
	</animation>
	<animation>
		<name>APU knob rotate</name>
		<type>rotate</type>
		<object-name>APUKnb</object-name>
		<property>controls/APU/off-start-run</property>
		<interpolation>
			<entry><ind>0</ind><dep>50</dep></entry>
			<entry><ind>1</ind><dep>-50</dep></entry>
			<entry><ind>2</ind><dep>0</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>-0.156</y-m>
			<z-m>-0.286</z-m>
		</center>
	</animation>

