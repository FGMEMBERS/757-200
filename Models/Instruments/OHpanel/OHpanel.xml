<?xml version="1.0" encoding="UTF-8" ?>

<!-- Boeing 757-200 for FlightGear -->
<!-- Overhead Panel -->

<PropertyList>

	<path>OHpanel.ac</path>

	<!-- ANIMATIONS -->
	<!-- Panel lighting -->
	<animation>
		<name>Panel lighting</name>
		<type>material</type>
		<object-name>Text</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>

	<!-- Battery button -->
	<animation>
		<name>Battery button pick</name>
		<type>pick</type>
		<object-name>BatteryBtn</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/electric/battery-switch</property>
			</binding>
		</action>
	</animation>
	<animation>
		<name>Battery button textranslate</name>
		<type>textranslate</type>
		<object-name>BatteryBtn</object-name>
		<property>controls/electric/battery-switch</property>
		<factor>0.5</factor>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- APU generator button -->
