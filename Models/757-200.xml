	<!-- ANIMATIONS -->
	<!-- Turbofan rotation -->
	<animation>
		<type>select</type>
		<object-name>LFanTurbofanDskS</object-name>
		<condition>
			<less-than>
				<property>engines/engine[0]/rpm</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>LFanTurbofanDskF</object-name>
		<condition>
			<greater-than-equals>
				<property>engines/engine[0]/rpm</property>
				<value>15</value>
			</greater-than-equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>RFanTurbofanDskS</object-name>
		<condition>
			<less-than>
				<property>engines/engine[1]/rpm</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>RFanTurbofanDskF</object-name>
		<condition>
			<greater-than-equals>
				<property>engines/engine[1]/rpm</property>
				<value>15</value>
			</greater-than-equals>
		</condition>
	</animation>

